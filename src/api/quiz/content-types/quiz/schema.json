    },
    "introformat": {
      "type": "integer"
    },
    "timelimit": {
      "type": "integer"
    },
    "grade": {
      "type": "integer"
    },
    "password": {
      "type": "string"
    },
    "questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question"
    },
    "quiz_grades": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz-grade.quiz-grade"
    },
    "timeopen": {
      "type": "datetime"
    },
    "timeclose": {
      "type": "datetime"
    }
  }
