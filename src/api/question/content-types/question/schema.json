    },
    "parent": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "modified": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "qtype": {
      "type": "string"
    },
    "qtype_multichoice_options": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::qtype-multichoice-option.qtype-multichoice-option"
