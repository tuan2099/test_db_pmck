    "question_answers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question-answer.question-answer"
    },
    "question_multianswers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question-multianswer.question-multianswer"
